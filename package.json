{
  "name": "dojo",
  "version": "1.1.1",
  "description": "personal node cli",
  "main": "index.js",
  "bin": {
    "mvc": "./bin/mvc.js"
  },
  "scripts": {
    "start": "node bin/mvc.js",
    "prepublishOnly": "yarn build && np --no-cleanup --yolo --any-branch",
    "postPublish": "yarn build:clear",
    "build": "rm -rf ./lib & tsc -m commonjs --outDir ./lib",
    "build:clear": "find ./lib -delete",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint '{lib,commands,actions}/**/*.ts' --fix"
  },
  "keywords": [
    "mvc"
  ],
  "author": "sam",
  "license": "ISC",
  "private": false,
  "devDependencies": {
    "chalk": "^4.0.0",
    "commander": "^5.0.0",
    "download-git-repo": "^3.0.2",
    "elint": "^2.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "gulp": "^4.0.2",
    "handlebars": "^4.7.6",
    "inquirer": "^7.1.0",
    "log-symbols": "^3.0.0",
    "np": "^6.2.1",
    "ora": "^4.0.3"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "fs-extra": "^10.0.0",
    "globby": "^11.0.3",
    "isbinaryfile": "^4.0.8",
    "javascript-stringify": "^2.1.0",
    "vue-codemod": "^0.0.5"
  }
}
