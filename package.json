{
  "name": "wyp",
  "version": "1.1.1",
  "description": "personal node cli",
  "main": "bin/wyp",
  "scripts": {
    "start": "ts-node bin/wyp.ts",
    "prepublishOnly": "yarn build && np --no-cleanup --yolo --any-branch",
    "postPublish": "yarn build:clear",
    "build": "rm -rf ./lib & tsc -m commonjs --outDir ./lib",
    "build:clear": "find ./lib -delete",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint '{lib,commands,actions}/**/*.ts' --fix"
  },
  "bin": {
    "wyp": "bin/wyp"
  },
  "keywords": [
    "wyp"
  ],
  "author": "sam",
  "license": "ISC",
  "private": false,
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.14.9",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.0.0",
    "commander": "^5.0.0",
    "download-git-repo": "^3.0.2",
    "eslint": "^7.21.0",
    "gulp": "^4.0.2",
    "handlebars": "^4.7.6",
    "inquirer": "^7.1.0",
    "log-symbols": "^3.0.0",
    "np": "^6.2.1",
    "ora": "^4.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
